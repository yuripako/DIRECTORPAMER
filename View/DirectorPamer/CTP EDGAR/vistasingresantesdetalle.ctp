			$(window).bind("load", function(){
				var remove_layer_pixel = function(str){
					var str = str.replace("px","");
					return parseFloat(str);
				};
				var get_value_pixel = function(obj, css){
					return remove_layer_pixel(obj.css(css));
				};
				var custom_form_dropdown = function(p1, p2, p3, p4, f1, fun){
					var calculate_select_width = function(obj){
						return obj.width() + get_value_pixel(obj,"paddingLeft") + get_value_pixel(obj,"paddingRight");
					};
					var calculate_select_top = function(obj){
						return obj.offset().top + obj.height() + get_value_pixel(obj,"borderWidth") + get_value_pixel(obj,"paddingTop") + get_value_pixel(obj,"paddingBottom") + 11;
					};
					var change_img_dropdown = function(obj){
						var img = obj.find("img");
						var a = "arriba";var b = "abajo";
						if(img.attr("src").indexOf(b) == -1){
							img.attr("src",img.attr("src").replace(a, b));
						}else{
							img.attr("src",img.attr("src").replace(b, a));
						}
					};
					var show_select_pixel = function(par, obj){
						var img = par.find("img");
						var tg = img.attr("src").indexOf("-abajo");
						tg = (tg == -1)?"none":"block";
						obj.attr("style","width:" + calculate_select_width(par) + "px;top:"+calculate_select_top(par)+"px;left:" + par.offset().left + "px;display:" + tg + ";");
						change_img_dropdown(par);
					};
					var hide_select_pixel = function(div, par, obj){
						change_img_dropdown(obj);
						par.hide();
						obj.find(".texto").text(div.text());
						obj.find(".texto").attr("key",div.attr("key"));
					};
					var create_select_pixel = function(hc, bf, ss, sl){
						$(hc + " " + bf + " " + ss).click(function(e){
							e.preventDefault();
							var t = $(this);
							if(f1["data"].length > 0){
								show_select_pixel($(this), t.closest(bf).find("div[custom=\""+sl+"\"]"));
							}
							
						});
						$(hc + " " + bf + " div[custom=\""+sl+"\"] div").click(function(e){
							var t = $(this);
							hide_select_pixel($(this), $(this).closest("div[custom=\""+sl+"\"]"), $(hc + " " + bf + " " + ss));
						});
						$(window).scroll(function(){
							$(hc + " " + bf + " div[custom=\""+sl+"\"]").hide();
							change_img_dropdown($(hc + " " + bf + " " + ss));
						});
					};
					p4 = p4.replace(".", "");
					if(f1["data"].length > 0){
						$.each(f1["data"], function(index, item){
							$(p1 + " " + p2 + " div[custom=\""+p4+"\"]").append("<div key=\""+item["key"]+"\">"+item["tex"]+"</div>");
						});
						create_select_pixel(p1, p2, p3, p4);
					}else{
						$(p1 + " " + p2 + " " + p3).addClass("desactivado");
					}
					if(typeof fun === "function"){
						fun(p1, p2, p3, p4);
					}
				};
				
